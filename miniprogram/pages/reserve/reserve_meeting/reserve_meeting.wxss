.navbar{
  width: 100%;
  height: 432rpx;
  background: linear-gradient(#FFB300,#FF8D5B);
  text-align: center;
  line-height: 250rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  top: 0;
}
.arrow{
  width: 50rpx;
  height: 50rpx;
  margin: 10rpx;
  position: absolute;
  left: 10rpx;
  top: 56rpx;
}
.steps{
  margin: auto;
  display: flex;
  justify-content:space-around;
  width: 90%;
}
.step{
  margin-top: 180rpx;
}
.num{
  color: #F96F33;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #FFFFFF;
  border: 4rpx solid #F96F33;
  border-radius: 50%;
  margin-bottom: 20rpx;
}
.deep{
  background: #FF8F55;
}
.txt0{
  width: 52rpx;
  height: 36rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 36rpx;
  color: #FFFFFF; 
}
.line{
  margin: 0 auto;
  background: #FFFFFF;
  width: 120%;
  height: 4rpx;
  position: relative;
  top: -92rpx;
  left: 100rpx;
}
.body{
  width: 100%;
  background: #ffffff;
  margin-top: -100rpx;
  border-radius: 26rpx 26rpx 0 0;
  z-index: 10;
}
.alltitle{
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 56rpx;
  color: #181818;
  margin-top: 56rpx;
  margin-left: 36rpx;
  margin-bottom: 40rpx;
}
.meetingnum{
  width: 90%;
  height: 67px;
  margin: 0 auto;
  background: #FFFFFF;
  border: 1px solid #707070;
}
.title{
  margin-left: 36rpx;
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 56rpx;
  color: #181818;
}
.txt{
  position: absolute;
  width: 90%;
  left: 36rpx;
  bottom: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #7B7B7B;
}