.navbar{
  width: 100%;
  height: 105px;
  background: linear-gradient(to right,#FFB300,#FF8D5B);
  text-align: center;
  line-height: 125px;
  font-size: 18px;
  color: #FFFFFF;
  /* position: fixed; */
}
.title{
  font-size :18px;
  color:#181818;
  padding: 15px;
}
.search-box{
  width: 100%;
  box-sizing: border-box;
  position: relative;
}
input{
  width: 280px;
  height: 35px;
  padding-left: 15px;
  border-radius: 18px 0 0 18px;
  margin-left: 20px;
  border: 1px solid #FFB301;
}
.btn{
  position: relative;
  top:-37px;
  left: 301px;
  padding: 0;
  font-size :15px;
  width: 60px!important;
  height: 37px;
  line-height: 37px;
  color: white;
  background-color: #FFD063;
  border-radius:0 18px 18px 0;
  display: inline-block;
}
image{
  width: 40px;
  height: 40px;
}
.item-all{
  display: flex;
}
.item{
  margin-left: 20px;
  display: flex;
  flex-direction: column;
}
.item:first-of-type{
  margin-right: 20px;
}

