.navbar{
  width: 100%;
  height:160rpx;
  background: linear-gradient(to right,#FFB300,#FF8D5B);
  text-align: center;
  line-height: 150rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  position: fixed;
  top: 0;
  z-index: -1;
}
.arrow{
  position: absolute;
  left: 10rpx;
  top: 55rpx;
}
.box{
  margin-top: 160rpx;
}
.box input{
  padding: 20rpx 5%;
}
.line{
  margin: 0 auto;
  background: #DDDDDD;
  width: 90%;
  height: 2rpx;
}
.box button{
  width: 30%;
  background: linear-gradient(to right,#FFB300,#FF8D5B);
  display: inline-block;
  margin: 10%;
}