.navbar{
  width: 100%;
  height:160rpx;
  background: linear-gradient(to right,#FFB300,#FF8D5B);
  text-align: center;
  line-height: 150rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  position: fixed;
  top: 0;
  z-index: -1;
}
.arrow{
  position: absolute;
  left: 10rpx;
  top: 55rpx;
}
.top{
  width: 100%;
  height: 80rpx;
  margin-top: 180rpx;
}
.grey{
  margin: 20rpx 0 20rpx 20rpx;
  padding:5rpx 10rpx;
  color: #414141;
  background: #FFFFFF;
  background: #E2E2E2;
  border-radius: 22rpx;
}
.gold{
  margin: 20rpx 0 20rpx 20rpx;
  padding:5rpx 10rpx;
  color: #FFC21C;
  background: #FFF6DD;
  border: 1px solid #FFC21C;
  border-radius: 22rpx;
}
.num{
  float: right;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #7B7B7B;
  margin-right: 36rpx;
}
.com{
  width: 90%;
  margin: 10rpx auto 0 ;
}
.content{
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #7B7B7B;
}
.line2{
  background: #DDDDDD;
  width: 90%;
  height: 2rpx;
  margin: 15rpx auto 20rpx;
}