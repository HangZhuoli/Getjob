#edit_router{
  font-size: 16px;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 22px;
  color: #000000;
}
.juniors_msg_box{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.juniors_msg_box>view{
  width: 700rpx;
  height: 116rpx;
  border-bottom: 1px solid #efefef;
  display: flex;
  align-items: center;
}
.juniors_msg_box>view:nth-child(1){
  height: 154rpx;
  border-bottom: 1px solid #efefef;
  justify-content: space-between;
}
.juniors_msg_box>view:nth-child(1)>image{
  width: 120rpx;
  height: 120rpx;
  border: 1px solid red;
  border-radius: 120rpx;
}
/*标题*/
.juniors_msg_box > view > text{
  /*字体样式继承问题，待解决*/
  font-size: 16px;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 22px;
  color: #000000;
  margin-right: 10px;
}
/*input*/
.juniors_msg_box>view>input{
  width: 600rpx;
}
/*select*/
.juniors_msg_box>view>.select{
  width: 540rpx;
  height: 100%;
  display: flex;
  align-items: center;
}
.edit_btn{
  width: 424rpx;
  height: 88rpx;
  position: absolute;
  bottom: 136rpx;
  left: calc(50% - 212rpx);
  background-color: #ffc21c;
  color: #ffffff;
  border-radius: 6px;
}
.edit_btn::after{
  border: none;
}